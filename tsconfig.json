{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["src", "react-upang-system/src/components/Admin", "react-upang-system/src/components/Student", "react-upang-system/src/components/Admin", "react-upang-system/src/App.tsx", "frontend/src/components/Admin/Activities.tsx", "frontend/src/components/Admin/People.tsx", "frontend/src/components/Admin/Lessons.tsx"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
